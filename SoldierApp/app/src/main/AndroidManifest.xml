<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.soldierapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_calculator"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_calculator_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CalenderView"></activity>
        <activity android:name=".SettingScreen" />
        <activity android:name=".SubtractVacation" />
        <activity android:name=".AddVacation" />
        <activity android:name=".CalVacation" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>